@model List<RegisterUserAssignment.Models.User>

@{
    ViewBag.Title = "AllUsers";
    string successMessage = TempData["SuccessMessage"] as string;
}

@if (!string.IsNullOrEmpty(successMessage))
{
    <div class="alert alert-success">@successMessage</div>
}

<h2>All Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>Profile Picture</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.Email</td>
            </tr>

        }
    </tbody>


</table>

@Html.ActionLink("Add New User", "Index", "Registration", null, new { @class = "btn btn-primary" })


